{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
        "include": "**/*.json", 
        "exclude": "**/node_modules/**" 
      },
      {
        "include": "data/**/*",
        "outDir": "dist"
      }
    ]
  }
}
